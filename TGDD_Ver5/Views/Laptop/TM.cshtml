<style type="text/css">
    .container-fluid{
        padding:0;
    }
    .top-banner {
        width: 1224px;
        margin-bottom: 15px;
        margin-top: 15px;
    }

    .btn-bothsides {
        background-color: #d8d8d8b3;
        width: 24px;
        height: 48px;
        border-radius: 4px;
    }

    .set__bothsides {
        margin-left: -74px;
        margin-right: -74px;
    }

    .quick-link {
        width: 1200px;
        background-color: #e0e0e0;
        padding: 15px 0px;
        border-radius: 13px;
        margin-bottom:20px;
    }

    .set__grid {
        grid-template-rows: 35px;
        padding: 10px 20px;
        display: grid;
        row-gap: 15px;
        grid-template-columns: repeat(8, 1fr);
    }

    .set__grid a:hover {
        border: 2px solid #2F80ED;
    }

    .box__quicklink--top {
        align-items: center;
        border-radius: 33px;
        background-color: #ffffff;
        justify-content: center;
        display: flex;
        width: 127.5px;
        height: 34px;
        padding: 0px 14px;
        margin-right: 17px;
        margin-bottom: 17px;
    }

    .set--img {
        width: 93.15px;
        height: 25px;
    }
    .carousel{
        margin:0px;
    }
</style>
@model IEnumerable<TGDD_Ver5.Models.TradeMark>
@{ 
    Layout = null;
}
<div class="container-fluid top-banner" style="width:1200px;">
    <div id="carouselExampleIndicators_Category" class="carousel slide">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="5" aria-label="Slide 6"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="6" aria-label="Slide 7"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="7" aria-label="Slide 8"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="8" aria-label="Slide 9"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="9" aria-label="Slide 10"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="https://cdn.tgdd.vn/2023/08/banner/b2s-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/07/banner/i3-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/07/banner/gaming-1200-300-1200x300-1.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/08/banner/HP-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/09/banner/mac-m1-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/08/banner/asus-1200-300-1200x300-1.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/08/banner/Lenovo-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/10/banner/acer-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/08/banner/msi-1200-300-1200x300.png" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="https://cdn.tgdd.vn/2023/10/banner/Nvidia-1200-628-1200x300.png" class="d-block w-100" alt="...">
            </div>
        </div>
        <button class="set__bothsides carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators_Category" data-bs-slide="prev">
            <span class="btn-bothsides carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="set__bothsides carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators_Category" data-bs-slide="next">
            <span class="btn-bothsides carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>
<div class="container-fluid quick-link">
    <div class="set__grid">
        @foreach (var item in Model)
        {
            if (item.Products != null)
            {
                <a class="box__quicklink--top" href="@Url.Action("TradeMark", "Laptop", new {tm =item.NameTM})">
                    <img class="set--img" src="@Url.Content(item.Logo)">
                </a>
            }

        }
    </div>
</div>
