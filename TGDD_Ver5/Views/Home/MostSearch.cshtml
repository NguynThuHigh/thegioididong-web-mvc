@model IEnumerable<TGDD_Ver5.Models.MostSearched>
    @{ 
        Layout = null;
    }

<style>
    .mostsearch a{
        color:black;
        text-decoration:none;
        font-size:15px;
    }
    .mostsearch{
        width:1200px;
        margin-top:30px;
    }
</style>
<div class="mostsearch container-fluid">
    <h1>TÌM KIẾM NHIỀU</h1>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Search", "Home", new {query = Url.Content(item.Keyword)})"><span style="margin-left:10px">@item.Keyword</span></a>
    }
</div>

