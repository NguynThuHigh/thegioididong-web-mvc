@model IEnumerable< TGDD_Ver5.Models.Banner>
    @{ 
        Layout = null;
    }
<link href="~/Content/Banner/Home_Carousel_1.css" rel="stylesheet" />
<style>
    #Home_Carousel_1 .main_slick {
        font-family: Arial;
        width: 1200px;
        display: block;
        margin: 0 auto;
        position: relative;
        top: -90px;
        margin-bottom: 25px;
    }
    #Home_Carousel_1 .slick-slide {
        margin: 0 16px;
    }
    #Home_Carousel_1 .slick-prev {
        left: -24px;
    }
    #Home_Carousel_1 .slick-next {
        right: -21px;
    }
</style>
<div id="Home_Carousel_1">
    <div class="main_slick container-fluid" style="top:-90px">
        <div class="slider slider-nav">
            @foreach (var item in Model)
            {
                <div class="slider__banner"><a href="@Url.Content(item.LinkBanner)"><img src="@Url.Content(item.ImgBanner)"></a></div>

            }

        </div>

    </div>
</div>

<script>
    document.addEventListener('DOMContentLoaded', function () {
      $('.slider-nav').slick({
        infinity: true,
        slidesToShow: 2,
        slidesToScroll: 2,
        focusOnSelect: true,
         autoplay: true,
        autoplaySpeed :2500,

      });
    });

</script>